<section class="s-block">
	<div class="container flex flex-col items-center justify-center store-features">
		<div class="w-7/12 mb-8">
			<h1 class="mb-5 leading-normal text-center text-gray-800 lg:text-5xl">{{component.title}}</h1>
			<div class="w-full h-1 bg-gray-400 rounded-lg lg:h-3"></div>
		</div>
		<div class="grid grid-cols-4 grid-feattures-about gap-y-10 gap-x-5">
			{% for link in component.brand %}
				<div class="flex flex-col items-center justify-center p-10 lg:p-0 ">
					<img src="{{link.image}}"/>
				</div>
			{% endfor %}
		</div>
	</div>
</section>
  