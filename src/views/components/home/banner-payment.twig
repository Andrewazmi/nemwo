<section class ="s-block">
	<div class="container">
		<div class="flex flex-col justify-center items-center store-features bg-[#F8F4F4] rounded-3xl p-5">
			<h1 class=" text-6xl"> {{component.title}}</h1>
			<div class="grid grid-cols-2 sm:grid-cols-4 gap-6 mt-8">
				{% for link in component.banner_payment %}
					<a href="{{link.url}}" class="flex flex-col justify-center items-center md:max-w-[270px] md:max-h-[180px]  max-w-[120px] max-h-[120px]">
							<img src="{{link.image}}" class="object-contain w-full h-full" />
					</a>
				{% endfor %}
			</div>
		</div>
	</div>
</section>